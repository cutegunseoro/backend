<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssafy.travlog.api.mapper.TravelMapper">
    <select id="selectTravelByTravelId" resultType="TravelModel">
        SELECT *
        FROM travel
        WHERE travel_id = #{travelId}
    </select>

    <select id="selectTravelsByMemberId" resultType="TravelModel">
        SELECT *
        FROM travel
        WHERE member_id = #{memberId}
    </select>

    <insert id="insertTravel" parameterType="TravelInsertModel">
        INSERT INTO travel (member_id, title, start_datetime, end_datetime)
        VALUES (#{memberId}, #{title}, #{startDatetime}, #{endDatetime})
    </insert>
</mapper>